<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Restaurants</title>
  </head>
  <body>
    @if(@Model.Count==0)
    {
      <p>
        No restaurants have been added yet.
      </p>
    }
    else
    {
      <ul>
      foreach(var restaurants in Model)
      {
        <li><a href="#">restaurants.GetName()</a></li>
      }
      </ul>
    }
  </body>
</html>
