<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>@Model['cuisine'].GetName()</title>
  </head>
  <body>
    <h2>Here are the restaurants that are @Model['cuisine'].GetName()</h2>
    <ul>
    @foreach (restaurant in Model['restaurants'])
    {
      <li><a href="/restaurant/@restaurant.GetId()">@restaurant.GetName()</a></li>
    }
  </ul>
  <a href="/">Return Home</a>
  </body>
</html>
